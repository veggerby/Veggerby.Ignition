{
  "Ignition": {
    "Policy": "BestEffort",
    "ExecutionMode": "Parallel",
    "GlobalTimeout": "00:00:30",
    "CancelOnGlobalTimeout": false,
    "CancelIndividualOnTimeout": true,
    "EnableTracing": true,
    "MaxDegreeOfParallelism": 4,
    "Timeouts": {
      "Database": "00:00:10",
      "ExternalApi": "00:00:08",
      "CacheWarmup": "00:00:15"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
