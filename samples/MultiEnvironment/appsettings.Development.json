{
  "Ignition": {
    "Policy": "BestEffort",
    "ExecutionMode": "Parallel",
    "GlobalTimeout": "00:00:20",
    "CancelOnGlobalTimeout": false,
    "CancelIndividualOnTimeout": false,
    "EnableTracing": true,
    "MaxDegreeOfParallelism": 8,
    "Timeouts": {
      "Database": "00:00:05",
      "ExternalApi": "00:00:03",
      "CacheWarmup": "00:00:05"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Veggerby.Ignition": "Debug"
    }
  }
}
